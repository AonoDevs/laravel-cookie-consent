{
    "name": "aonodevs/laravel-cookie-consent",
    "description": "Make your Laravel app comply with the crazy EU cookie law",
    "keywords": [
        "aonodevs",
        "cookie-consent",
        "cookie",
        "laravel",
        "refuse",
        "comply",
        "law",
        "eu"
    ],
    "homepage": "https://github.com/aonodevs/laravel-cookie-consent",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Houssa",
            "email": "lucas@retinens.com",
            "homepage": "https://retinens.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^8.77|^9.0|^10.0",
        "illuminate/view": "^8.77|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "fakerphp/faker": "~1.9",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Retinens\\CookieConsent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Retinens\\CookieConsent\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Retinens\\CookieConsent\\CookieConsentServiceProvider"
            ],
            "aliases": {
                "CookieConsent": "Retinens\\CookieConsent\\Facades\\CookieConsent"
            }
        }
    },
    "prefer-stable": true
}
